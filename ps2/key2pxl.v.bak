		8'h34: out = G;
		8'h31: out = N;
		8'h42: out = K;
		8'h23: out = D;
		8'h4D: out = P;
		8'h55: out = Plus;
		8'h4E: out = Minus;
		default:
			out = 256'd0;
	endcase 

	o_one_pixel = out[{i_Y[4:1], i_X[4:1]}];

end


